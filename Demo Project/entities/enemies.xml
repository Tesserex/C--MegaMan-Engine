		<State name="Left">
			<Movement>
				<X direction="Left" />
			</Movement>
			<Trigger condition="StateTime>4 And (Collision.BlockBottomWidth &lt; 16 and Movement.Direction == Left) Or Collision.BlockLeft == True">
				<Effect><State>Start</State></Effect>
