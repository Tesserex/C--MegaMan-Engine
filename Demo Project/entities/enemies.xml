			<Movement>
				<Flying>True</Flying>
				<X magnitude="0.5" direction="Right" />
			</Movement>
			<Collision>
				<EnableBox name="H" />
			</Collision>
			<Trigger condition="StateTime>4 And (Collision.BlockBottomWidth &lt; 16 and Movement.Direction == Right) Or Collision.BlockRight == True">
				<Effect><State>Left</State></Effect>
